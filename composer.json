{
  "name": "161io/labodata-prestashop",
  "description": "Module LaboData pour Prestashop",
  "type": "library",
  "license": "(c) 161 SARL",
  "keywords": [
    "prestashop", "module", "database", "labodata", "pharmacy"
  ],
  "homepage": "https://www.labodata.com",
  "config": {
    "autoloader-suffix": "LaboDataPrestashop",
    "platform-check": false
  },
  "autoload": {
    "psr-4": {
      "LaboDataPrestaShop\\": "src/LaboDataPrestaShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaboDataPrestaShopTest\\": "src/LaboDataPrestaShopTest/"
    }
  },
  "require": {
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0"
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
